    "@types/mocha": "^9",
    "@types/sinon": "^10",
    "@types/node": "^22",
    "sinon": "^19",
    "mocha-junit-reporter": "^2",
    "mocha": "^10",
    "ts-node": "^10",
    "eslint-config-turbo": "^2",
    "nyc": "^15"
  },
  "packageManager": "npm@10.9.0"
}
